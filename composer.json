{
    "name": "dis-fx/fx-router",
    "description": "100% proprietary router - zero third-party dependencies",
    "type": "library",
    "license": "proprietary",
    "homepage": "https://github.com/dis-fx/fx-router",
    "support": {
        "email": "framework@digitalintelligentsolutions.om"
    },
    "require": {
        "dis-fx/fx-nucleus": "^0.1.0|dev-master"
    },
    "require-dev": {},
    "autoload": {
        "psr-4": {
            "Dis\\FictionX\\Router\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "conflict": {
        "symfony/symfony": "*",
        "symfony/http-kernel": "*",
        "symfony/routing": "*",
        "illuminate/support": "*",
        "illuminate/routing": "*",
        "illuminate/http": "*",
        "monolog/monolog": "*"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": false
    }
}
